        <attr format="reference|color" name="VcConductColor"/>
        <attr format="reference|color" name="VcEndColor"/>
        <attr format="reference|string" name="VcEndText"/>
        <attr format="reference|string" name="VcCompany"/>
        <attr format="integer" name="VcDuration"/>
        <attr format="integer" name="VcIntervalTime"/>
