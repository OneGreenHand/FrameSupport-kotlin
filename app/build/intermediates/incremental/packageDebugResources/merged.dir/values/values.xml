    <style name="Common_Check_Box_Style" parent="Theme.AppCompat.Light">
        <item name="colorControlNormal">@color/c_DADADA</item>
        <item name="colorControlActivated">@color/colorPrimary</item>
    </style>
    <style name="DividerX">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.7dp</item>
        <item name="android:background">@color/c_DADADA</item>
    </style>
    <style name="DividerY">
        <item name="android:layout_width">0.7dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/c_DADADA</item>
    </style>
    <style name="EditText">
        <item name="android:textCursorDrawable">@drawable/shape_input_cursor</item>
        <item name="android:background">@null</item>
        <!-- 适配RTL -->
        <item name="android:textDirection">locale</item>
    </style>
    <style name="StartPage" parent="AppTheme">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowBackground">@color/colorPrimary</item>
    </style>
    <style name="TabLayoutTextStyle" parent="TextAppearance.Design.Tab">
        <item name="android:textSize">14sp</item>
        <item name="textAllCaps">false</item>
        <item name="android:textAllCaps">false</item>
    </style>
    <style name="wx_theme" parent="AppTheme">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>
    <declare-styleable name="AnimDownloadProgressButton">
        <attr format="float" name="progressbtn_radius"/>
        <attr format="reference|color" name="progressbtn_background_color"/>
        <attr format="reference|color" name="progressbtn_background_second_color"/>
        <attr format="reference|string" name="progressbtn_text"/>
        <attr format="float" name="progressbtn_text_size"/>
        <attr format="reference|color" name="progressbtn_text_color"/>
        <attr format="reference|color" name="progressbtn_text_covercolor"/>
    </declare-styleable>
    <declare-styleable name="CircleImageView">
        <attr format="dimension" name="civ_border_width"/>
        <attr format="color" name="civ_border_color"/>
        <attr format="boolean" name="civ_border_overlay"/>
        <attr format="color" name="civ_circle_background_color"/>
    </declare-styleable>
    <declare-styleable name="RoundedImageView">
        <attr format="dimension" name="riv_corner_radius"/>
        <attr format="dimension" name="riv_corner_radius_top_left"/>
        <attr format="dimension" name="riv_corner_radius_top_right"/>
        <attr format="dimension" name="riv_corner_radius_bottom_left"/>
        <attr format="dimension" name="riv_corner_radius_bottom_right"/>
        <attr format="dimension" name="riv_border_width"/>
        <attr format="color" name="riv_border_color"/>
        <attr format="boolean" name="riv_mutate_background"/>
        <attr format="boolean" name="riv_oval"/>
        <attr name="riv_scaleType">
            <enum name="matrix" value="0"/>
            <enum name="fitXY" value="1"/>
            <enum name="fitStart" value="2"/>
            <enum name="fitCenter" value="3"/>
            <enum name="fitEnd" value="4"/>
            <enum name="center" value="5"/>
            <enum name="centerCrop" value="6"/>
            <enum name="centerInside" value="7"/>
        </attr>
        <attr name="riv_tile_mode">
            <enum name="clamp" value="0"/>
            <enum name="repeat" value="1"/>
            <enum name="mirror" value="2"/>
        </attr>
        <attr name="riv_tile_mode_x">
            <enum name="clamp" value="0"/>
            <enum name="repeat" value="1"/>
            <enum name="mirror" value="2"/>
        </attr>
        <attr name="riv_tile_mode_y">
            <enum name="clamp" value="0"/>
            <enum name="repeat" value="1"/>
            <enum name="mirror" value="2"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="TitleBarLayout">
        <attr format="reference|color" name="TblBackgroundColor"/>
        <attr format="reference|string" name="TblText"/>
        <attr format="reference|color" name="TblTextColor"/>
        <attr format="boolean" name="TblRTextShow"/>
        <attr format="reference|string" name="TblRText"/>
        <attr format="reference|color" name="TblRTextColor"/>
        <attr format="boolean" name="TblBackShow"/>
        <attr format="reference" name="TblBackImage"/>
    </declare-styleable>
